"  count = 1\n","  Factors = [CorrOpPreds(Boxscore)['Col Name'][0]]\n","  while len(Factors) < 5:\n","    check = 0\n","    for factor in Factors:\n","      if correlation_matrix.loc[factor,CorrOpPreds(Boxscore)['Col Name'][count]] > 0.6:\n","        check = 1\n","    if check == 0:\n","      Factors.append(CorrOpPreds(Boxscore)['Col Name'][count])\n","    count += 1\n","\n","  Predictors = X[Factors]\n","  print(Factors)\n","  scaler = StandardScaler()\n","  ScaledData = scaler.fit_transform(Predictors)\n","  ScaledExpFrame = pd.DataFrame(ScaledData, columns = Factors)\n","  unique_seed = 1973\n","  X_train, X_test, y_train, y_test = train_test_split(ScaledExpFrame, y, test_size = 0.2, random_state=unique_seed)\n","  log_reg_single = LogisticRegression(random_state=unique_seed, max_iter=500)\n","  log_reg_single.fit(X_train, y_train)\n","  y_pred = log_reg_single.predict(X_test)\n","  y_prob = log_reg_single.predict_proba(X_test)[:, 1]\n","  results = pd.DataFrame({\n","    'Win Predictions': y_pred,\n","    'Actual': y_test,\n","    'Probabilities': y_prob})\n","\n","  print(results)\n","  #creating confusion matrix based off of the tested and predicted dependent variables\n","  conf_matrix_single = confusion_matrix(y_test, y_pred)\n","  sns.heatmap(conf_matrix_single, annot=True, fmt=\"d\", cmap=\"Blues\",\n","              xticklabels=[\"0 (Lost Game)\", \"1 (Won Game)\"],\n","              yticklabels=[\"0 (Lost Game)\", \"1 (Won Game)\"])\n","\n","  # Add title and labels\n","  plt.title(\"Confusion Matrix\")\n","  plt.xlabel(\"Predicted\")\n","  plt.ylabel(\"Actual\")\n","  plt.show()"],"metadata":{"id":"RJGx6tgLY60S","executionInfo":{"status":"ok","timestamp":1745450460175,"user_tz":240,"elapsed":19,"user":{"displayName":"Max Drogin","userId":"14072875463726990849"}}},"execution_count":4,"outputs":[]},{"cell_type":"code","source":["#Shapely Analysis\n","def Shapely(BoxScore,Factors):\n","  yName = 'score Rslt'\n","  xNames = Factors\n","  results = relativeImp(BoxScore, outcomeName = yName, driverNames = xNames)\n","  results = results.sort_values(by='rawRelaImpt', ascending=False)\n","  return results"],"metadata":{"id":"7sjX0_f5ZOrI","executionInfo":{"status":"ok","timestamp":1745450460324,"user_tz":240,"elapsed":7,"user":{"displayName":"Max Drogin","userId":"14072875463726990849"}}},"execution_count":5,"outputs":[]},{"cell_type":"markdown","source":["## Now Load in Dataframes and process"],"metadata":{"id":"ynqr6XUtZ7lJ"}},{"cell_type":"code","source":["#Knicks\n","df = pd.read_csv('Knicks_Season.csv')\n","\n","df.drop(columns=['Date', 'Rk', 'Gtm', 'H/@', 'score OT', 'Opp', 'score Opp', 'score Tm'], inplace=True)\n","\n","df['score Rslt'] = df['score Rslt'].map({'W': 1, 'L': 0})\n","\n","df = df.dropna()"],"metadata":{"id":"GEoIpAdzZ-YR","executionInfo":{"status":"ok","timestamp":1745450465950,"user_tz":240,"elapsed":102,"user":{"displayName":"Max Drogin","userId":"14072875463726990849"}}},"execution_count":6,"outputs":[]},{"cell_type":"code","source":["#Cavs\n","dfca = pd.read_csv('Cavs_Season.csv')\n","\n","dfca.drop(columns=['Date', 'Rk', 'Gtm', 'H/@', 'score OT', 'Opp', 'score Opp', 'score Tm'], inplace=True)\n","\n","dfca['score Rslt'] = dfca['score Rslt'].map({'W': 1, 'L': 0})\n","\n","dfca = dfca.dropna()"],"metadata":{"id":"beyrVBh0af2Q","executionInfo":{"status":"ok","timestamp":1745358920048,"user_tz":240,"elapsed":10,"user":{"displayName":"Max Drogin","userId":"14072875463726990849"}}},"execution_count":7,"outputs":[]},{"cell_type":"code","source":["#Celtics\n","dfc = pd.read_csv('Celtics_Season.csv')\n","\n","dfc.drop(columns=['Date', 'Rk', 'Gtm', 'H/@', 'score OT', 'Opp', 'score Opp', 'score Tm'], inplace=True)\n","\n","dfc['score Rslt'] = dfc['score Rslt'].map({'W': 1, 'L': 0})\n","\n","dfc = dfc.dropna()"],"metadata":{"id":"44WQ77WjacQI","executionInfo":{"status":"ok","timestamp":1745358920693,"user_tz":240,"elapsed":9,"user":{"displayName":"Max Drogin","userId":"14072875463726990849"}}},"execution_co